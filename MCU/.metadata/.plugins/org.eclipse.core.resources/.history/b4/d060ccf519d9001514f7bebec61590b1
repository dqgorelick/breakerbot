#include "mcu_api.h"
#include "mcu_errno.h"
/*
* This program will blink GPIO 48, which connects to Edison Arduino board DIG 7
*/

//convert Arduino ports to Intel MCU ports


void mcu_main()
{
    /* your configuration code starts here */
    gpio_setup(48, 1);  /* set GPIO 48 as output */
    gpio_setup(182, 1);
    gpio_setup(13, 1);
    gpio_setup(129, 1);

    while (1)
    {
        gpio_write(48, 1);
        gpio_write(182, 0);
        gpio_write(13, 0);
        gpio_write(129, 0);
        mcu_sleep(100);         /* sleep 1 second */
        gpio_write(48, 0);
		gpio_write(182, 1);
		gpio_write(13, 0);
		gpio_write(129, 0);
		mcu_sleep(100);
		gpio_write(48, 0);
		gpio_write(182, 0);
		gpio_write(13, 1);
		gpio_write(129, 0);
		mcu_sleep(100);
		gpio_write(48, 0);
		gpio_write(182, 0);
		gpio_write(13, 0);
		gpio_write(129, 1);
		mcu_sleep(100);
    }
}
